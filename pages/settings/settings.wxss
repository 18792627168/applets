.page-head{
  /* padding: 60rpx 50rpx 80rpx; */
  text-align: center;
}
.page-head-title{
  display: inline-block;
  /* padding: 0 40rpx 20rpx 40rpx; */
  font-size: 32rpx;
  color: #BEBEBE;
}
.page-head-line{
  margin: 0 auto;
  width: 150rpx;
  height: 2rpx;
  background-color: #D8D8D8;
}
.page-head-desc{
  padding: 0 40rpx 20rpx 40rpx;
  color: #9B9B9B;
  font-size: 44rpx;
}

.page-body{
  width: 100%;
  flex-grow: 1;
  overflow: hidden;
}
.touch-item{
  display: flex;
  padding: 10px;
}
.content{
  width: 100%;
  padding: 10rpx 50rpx;
  border-bottom:1px solid #ccc;
}
.titlecrep{
  text-align: center;
  font-size: 40rpx;
  color: #000000;
}